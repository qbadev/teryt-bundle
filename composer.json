{
    "name": "qbadev/teryt-bundle",
    "type": "symfony-bundle",
    "description": "Symfony TerytBundle for Symfony2",
    "keywords": ["teryt"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kuba Wolszczak",
            "email": "kuba@hotaru.pl"
        }
